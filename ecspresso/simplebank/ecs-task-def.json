{
  "containerDefinitions": [
    {
      "cpu": 128,
      "essential": true,
      "healthCheck": {
        "command": [
          "CMD-SHELL",
          "wget --quiet --spider http://localhost:3000/healthz || exit 1"
        ],
        "interval": 10,
        "retries": 3,
        "startPeriod": 60,
        "timeout": 5
      },
      "image": "774533787977.dkr.ecr.us-east-2.amazonaws.com/simplebank/app:{{ must_env `IMAGE_TAG` }}",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "simplebank-8a7e228",
          "awslogs-region": "us-east-2",
          "awslogs-stream-prefix": "simplebank-app"
        }
      },
      "memory": 512,
      "name": "simplebank-app",
      "portMappings": [
        {
          "appProtocol": "",
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp"
        }
      ],
      "secrets": [
        {
          "name": "DB_SOURCE",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:774533787977:secret:simplebank-rds-url-75f4d86-BDELgW"
        }
      ]
    }
  ],
  "cpu": "256",
  "executionRoleArn": "arn:aws:iam::774533787977:role/task-exec-role-b9c0082",
  "family": "simplebank-406c1da0",
  "ipcMode": "",
  "memory": "512",
  "networkMode": "awsvpc",
  "pidMode": "",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "tags": [
    {
      "key": "Project",
      "value": "simplebank"
    },
    {
      "key": "Environment",
      "value": "Dev"
    }
  ],
  "taskRoleArn": "arn:aws:iam::774533787977:role/simplebank-task-965d9ff"
}
